# Git LFS for large assets

# Data files
MiniProject/data/**/*.csv filter=lfs diff=lfs merge=lfs -text
MiniProject/data/**/*.json filter=lfs diff=lfs merge=lfs -text
# Reports (documents)
MiniProject/reports/**/*.pdf filter=lfs diff=lfs merge=lfs -text
MiniProject/reports/**/*.docx filter=lfs diff=lfs merge=lfs -text
MiniProject/reports/**/*.pptx filter=lfs diff=lfs merge=lfs -text
# Figures & images
MiniProject/**/figures/**/*.png filter=lfs diff=lfs merge=lfs -text
MiniProject/**/figures/**/*.jpg filter=lfs diff=lfs merge=lfs -text
MiniProject/**/figures/**/*.jpeg filter=lfs diff=lfs merge=lfs -text
MiniProject/reports/assets/readme/*.png filter=lfs diff=lfs merge=lfs -text
# Optional: large CSV results in artifacts
MiniProject/artifacts/**/*.csv filter=lfs diff=lfs merge=lfs -text
